<html>
<body>
</body>
<div>
   <header class="banner">
      <div class="banner-wrapper">
         <div class="banner-title">
            <!-- <a href="/support/home"><img src="/images/logo.png" class="portal-logo"></a> -->
            <h1 class="ellipsis heading">FixNix GRC</h1>
         </div>
         <nav class="banner-nav">
           <b><a href="sign_up"><b>Sign Up</b></a></b> &nbsp;<b>
        </nav>
     </div>
  </header>

</div>
<br>
<br>
<br>
<br>
<div class="row-fluid">
		<div class="offset4 span9">
			<div class="row-fluid">
				<div class="span6">
					<div class="well">
						<div class="well-header">
							<h2 class="centered">Login</h2>
						</div>
						<div class="inner well">
							<%= form_tag sessions_path do %>
							<p>
								<%= label_tag :email %>
								<%= text_field_tag :email, params[:email] %>
							</p>
							<p>
								<%= label_tag :password %>
								<%= password_field_tag :password %>
							</p>
							<br>
							<button class="btn btn-primary btn-block">Login</p></button>
							<% end %>

						</div>
					</div>
				</div>
			</div>
		</div>
	
</div>
</div>